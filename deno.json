{
  "imports": {
    "lume/": "https://deno.land/x/lume/"
  },
  "lock": false,
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "dev": "deno task lume -s"
  },
  "lint": {
    "rules": {
      "exclude": [
        "jsx-key"
      ]
    }
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ],
    "jsx": "precompile",
    "jsxImportSource": "npm:preact"
  }
}
